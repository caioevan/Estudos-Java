package exerciciosTeste;

import java.util.Arrays;
import java.util.Scanner;

public class Fibonacci {

	public static void main(String[] args) {
		Scanner entrada = new Scanner(System.in);
		
		int n = 0;
		
		do {
			System.out.println("Informe um número inteiro menor que 46 e maior que 0: ");
			n = entrada.nextInt();
			if(n >= 46 || n <= 0) {
				System.out.println("O número não pode ser menor que 46 e maior que 0!");
			}
		}while(n >= 46 || n <= 0);
		
		
		for(int i = 0; i == n; i++) {
			int[] f = new int[n];
			f[i] = i + (i--);
			if(f[0] == -1) {
				f[0]++;
			}
			System.out.print(f[i] + " ");
		}
		
		entrada.close();
	}
}
