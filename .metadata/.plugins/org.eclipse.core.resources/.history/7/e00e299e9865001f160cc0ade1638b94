package exerciciosTeste;

import java.util.Arrays;
import java.util.Scanner;

public class Fibonacci {

	public static void main(String[] args) {
		Scanner entrada = new Scanner(System.in);
		
		int n = 0;
		
		do {
			System.out.println("Informe um número inteiro menor que 46 e maior que 0: ");
			n = entrada.nextInt();
			if(n >= 46 || n <= 0) {
				System.out.println("O número não pode ser menor que 46 e maior que 0!");
			}
		}while(n >= 46 || n <= 0);
		
		int[] f = new int[n];
		
		for(int i = 0; i == n; i++) {
			f[i] = i + 1;
		}
		
		System.out.println(Arrays.toString(f));
		
		entrada.close();
	}
}
